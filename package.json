{
  "name": "k-pop",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "serve:demo": "vue-cli-service serve demo/main.js",
    "serve": "vue-cli-service serve",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "build:bili": "bili --format umd --format umd-min --format es --module-name KPop --env.NODE_ENV production -d package/dist",
